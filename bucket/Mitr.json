{
    "version": "nightly",
    "description": "A formal Loopless Thai and Sans Latin design. The name 'Mitr' means friend.",
    "homepage": "http://github.com/cadsondemak/mitr",
    "license": "OFL-1.1",
    "depends": "scoop-nerd-fonts-helper",
    "url": "https://fonts.google.com/download?family=Mitr#/fonts.zip",
    "installer": {
        "script": [
            "try {",
            "    scoop-nerd-fonts-helper -Command install",
            "} catch {",
            "    abort($_, 1)",
            "}"
        ]
    },
    "uninstaller": {
        "script": [
            "try {",
            "    scoop-nerd-fonts-helper -Command uninstall",
            "} catch {",
            "    abort($_, 1)",
            "}"
        ]
    }
}
