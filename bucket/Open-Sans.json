{
    "version": "nightly",
    "description": "Humanist sans-serif typeface.",
    "homepage": "https://fonts.google.com/specimen/Open+Sans",
    "license": "Apache-2.0",
    "depends": "scoop-nerd-fonts-helper",
    "url": "https://fonts.google.com/download?family=Open+Sans#/fonts.zip",
    "installer": {
        "script": [
            "try {",
            "    scoop-nerd-fonts-helper -Command install",
            "} catch {",
            "    abort($_, 1)",
            "}"
        ]
    },
    "uninstaller": {
        "script": [
            "try {",
            "    scoop-nerd-fonts-helper -Command uninstall",
            "} catch {",
            "    abort($_, 1)",
            "}"
        ]
    }
}
