{
    "version": "nightly",
    "description": "Thai National Font. The name 'Chakra Petch' means a crystal disc.",
    "homepage": "https://fonts.google.com/specimen/Chakra+Petch",
    "license": "OFL-1.1",
    "depends": "scoop-nerd-fonts-helper",
    "url": "https://fonts.google.com/download?family=Chakra%20Petch#/fonts.zip",
    "installer": {
        "script": [
            "try {",
            "    scoop-nerd-fonts-helper -Command install",
            "} catch {",
            "    abort($_, 1)",
            "}"
        ]
    },
    "uninstaller": {
        "script": [
            "try {",
            "    scoop-nerd-fonts-helper -Command uninstall",
            "} catch {",
            "    abort($_, 1)",
            "}"
        ]
    }
}
