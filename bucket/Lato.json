{
    "version": "nightly",
    "description": "Sans-serif typeface family. The name 'Lato' means summer in Polish.",
    "homepage": "https://www.latofonts.com/",
    "license": "OFL-1.1",
    "depends": "scoop-nerd-fonts-helper",
    "url": "https://www.latofonts.com/download/lato2ofl-zip#/fonts.zip",
    "extract_dir": "Lato2OFL",
    "installer": {
        "script": [
            "try {",
            "    scoop-nerd-fonts-helper -Command install",
            "} catch {",
            "    abort($_, 1)",
            "}"
        ]
    },
    "uninstaller": {
        "script": [
            "try {",
            "    scoop-nerd-fonts-helper -Command uninstall",
            "} catch {",
            "    abort($_, 1)",
            "}"
        ]
    }
}
