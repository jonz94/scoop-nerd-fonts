    "license": "MIT",
    "depends": "scoop-nerd-fonts-helper",
    "url": " ",
    "hash": " ",
    "installer": {
        "script": [
            "try {",
            "    scoop-nerd-fonts-helper -Command install -Filter '*Complete Windows Compatible.*'",
            "} catch {",
            "    abort($_, 1)",
            "}"
        ]
