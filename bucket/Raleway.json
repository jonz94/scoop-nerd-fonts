{
    "version": "nightly",
    "description": "Elegant sans-serif typeface family.",
    "homepage": "https://github.com/impallari/Raleway/",
    "license": "OFL-1.1",
    "depends": "scoop-nerd-fonts-helper",
    "url": "https://fonts.google.com/download?family=Raleway#/fonts.zip",
    "installer": {
        "script": [
            "try {",
            "    scoop-nerd-fonts-helper -Command install",
            "} catch {",
            "    abort($_, 1)",
            "}"
        ]
    },
    "uninstaller": {
        "script": [
            "try {",
            "    scoop-nerd-fonts-helper -Command uninstall",
            "} catch {",
            "    abort($_, 1)",
            "}"
        ]
    }
}
